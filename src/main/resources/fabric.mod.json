  "icon": "assets/createchem/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "ml.pluto7073.createchem.CreateChemistry"
    ],
    "client": [
      "ml.pluto7073.createchem.client.CreateChemistryClient"
    ],
    "modmenu": [
      "ml.pluto7073.createchem.modmenu.CreateChemModMenuImpl"
    ]
  },
